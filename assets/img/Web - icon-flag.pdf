<html dir="ltr" mozdisallowselectionprint=""><head>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>Web - icon-flag.pdf</title>

<!-- This snippet is used in firefox extension, see Makefile -->
<!-- base href="resource://pdf.js/web/" -->
<script type="text/javascript" src="Web%20-%20icon-flag_files/l10n.js"></script>
<script type="text/javascript" src="Web%20-%20icon-flag_files/pdf.js"></script>


    <link rel="stylesheet" href="Web%20-%20icon-flag_files/viewer.css">






    <script type="text/javascript" src="Web%20-%20icon-flag_files/debugger.js"></script>
    <script type="text/javascript" src="Web%20-%20icon-flag_files/viewer.js"></script>
  </head>

  <body>
    <div id="outerContainer">

      <div id="sidebarContainer">
        <div id="toolbarSidebar">
          <div class="splitToolbarButton toggled">
            <button id="viewThumbnail" class="toolbarButton group toggled" title="Показать миниатюры" tabindex="1" data-l10n-id="thumbs">
               <span data-l10n-id="thumbs_label">Миниатюры</span>
            </button>
            <button id="viewOutline" class="toolbarButton group" title="Показать содержание документа" tabindex="2" data-l10n-id="outline">
               <span data-l10n-id="outline_label">Содержание документа</span>
            </button>
          </div>
        </div>
        <div id="sidebarContent">
          <div id="thumbnailView"><a title="Страница 1" href="http://fontawesome.veliovgroup.com/assets/img/icon-flag.pdf#page=1"><div data-loaded="true" class="thumbnail" id="thumbnailContainer1"><div style="width: 98px; height: 91.4667px;" class="thumbnailSelectionRing"><canvas aria-label="Миниатюра страницы 1" class="thumbnailImage" height="91" width="98" moz-opaque="" id="thumbnail1"></canvas></div></div></a></div>
          <div id="outlineView" class="hidden"><div class="noOutline">Содержание не доступно</div></div>
        </div>
      </div>  <!-- sidebarContainer -->

      <div id="mainContainer">
        <div class="findbar hidden doorHanger" id="findbar">
          <label for="findInput" class="toolbarLabel" data-l10n-id="find_label">Найти:</label>
          <input id="findInput" class="toolbarField" tabindex="20">
          <div class="splitToolbarButton">
            <button class="toolbarButton findPrevious" title="Найти предыдущее вхождение фразы в текст" id="findPrevious" tabindex="21" data-l10n-id="find_previous">
              <span data-l10n-id="find_previous_label">Назад</span>
            </button>
            <div class="splitToolbarButtonSeparator"></div>
            <button class="toolbarButton findNext" title="Найти следующее вхождение фразы в текст" id="findNext" tabindex="22" data-l10n-id="find_next">
              <span data-l10n-id="find_next_label">Далее</span>
            </button>
          </div>
          <input id="findHighlightAll" class="toolbarField" type="checkbox">
          <label for="findHighlightAll" class="toolbarLabel" tabindex="23" data-l10n-id="find_highlight">Подсветить все</label>
          <input id="findMatchCase" class="toolbarField" type="checkbox">
          <label for="findMatchCase" class="toolbarLabel" tabindex="24" data-l10n-id="find_match_case_label">С учётом регистра</label>
          <span id="findMsg" class="toolbarLabel"></span>
        </div>
        <div class="toolbar">
          <div id="toolbarContainer">
            <div id="toolbarViewer">
              <div id="toolbarViewerLeft">
                <button id="sidebarToggle" class="toolbarButton" title="Открыть/закрыть панель навигации" tabindex="3" data-l10n-id="toggle_slider">
                  <span data-l10n-id="toggle_slider_label">Панель навигации</span>
                </button>
                <div class="toolbarButtonSpacer"></div>
                <button id="viewFind" class="toolbarButton group hidden" title="Найти в документе" tabindex="4" data-l10n-id="findbar">
                   <span data-l10n-id="findbar_label">Найти</span>
                </button>
                <div class="splitToolbarButton">
                  <button disabled="disabled" class="toolbarButton pageUp" title="Предыдущая страница" id="previous" tabindex="5" data-l10n-id="previous">
                    <span data-l10n-id="previous_label">Предыдущая</span>
                  </button>
                  <div class="splitToolbarButtonSeparator"></div>
                  <button disabled="disabled" class="toolbarButton pageDown" title="Следующая страница" id="next" tabindex="6" data-l10n-id="next">
                    <span data-l10n-id="next_label">Следующая</span>
                  </button>
                </div>
                <label id="pageNumberLabel" class="toolbarLabel" for="pageNumber" data-l10n-id="page_label">Страница:</label>
                <input max="1" id="pageNumber" class="toolbarField pageNumber" value="1" size="4" min="1" tabindex="7" type="number">
                
                <span id="numPages" class="toolbarLabel">из 1</span>
              </div>
              <div id="toolbarViewerRight">
                <input id="fileInput" class="fileInput" oncontextmenu="return false;" style="visibility: hidden; position: fixed; right: 0; top: 0" type="file">


                <button id="fullscreen" class="toolbarButton fullscreen" title="Перейти в режим презентации" tabindex="11" data-l10n-id="presentation_mode">
                  <span data-l10n-id="presentation_mode_label">Режим презентации</span>
                </button>

                <button id="openFile" class="toolbarButton openFile" title="Открыть файл" tabindex="12" data-l10n-id="open_file" hidden="true">
                   <span data-l10n-id="open_file_label">Открыть</span>
                </button>

                <button id="print" class="toolbarButton print" title="Печать" tabindex="13" data-l10n-id="print">
                  <span data-l10n-id="print_label">Печать</span>
                </button>

                <button id="download" class="toolbarButton download" title="Загрузить" tabindex="14" data-l10n-id="download">
                  <span data-l10n-id="download_label">Загрузить</span>
                </button>
                <!-- <div class="toolbarButtonSpacer"></div> -->
                <a href="http://fontawesome.veliovgroup.com/assets/img/icon-flag.pdf#page=1&amp;zoom=auto,0,63" id="viewBookmark" class="toolbarButton bookmark" title="Ссылка на текущий вид (скопировать или открыть в новом окне)" tabindex="15" data-l10n-id="bookmark"><span data-l10n-id="bookmark_label">Текущий вид</span></a>
              </div>
              <div class="outerCenter">
                <div class="innerCenter" id="toolbarViewerMiddle">
                  <div class="splitToolbarButton">
                    <button class="toolbarButton zoomOut" title="Уменьшить" tabindex="8" data-l10n-id="zoom_out">
                      <span data-l10n-id="zoom_out_label">Уменьшить</span>
                    </button>
                    <div class="splitToolbarButtonSeparator"></div>
                    <button class="toolbarButton zoomIn" title="Увеличить" tabindex="9" data-l10n-id="zoom_in">
                      <span data-l10n-id="zoom_in_label">Увеличить</span>
                     </button>
                  </div>
                  <span id="scaleSelectContainer" class="dropdownToolbarButton">
                     <select id="scaleSelect" title="Масштаб" oncontextmenu="return false;" tabindex="10" data-l10n-id="zoom">
                      <option id="pageAutoOption" value="auto" selected="selected" data-l10n-id="page_scale_auto">Автоматически</option>
                      <option id="pageActualOption" value="page-actual" data-l10n-id="page_scale_actual">Реальный размер</option>
                      <option id="pageFitOption" value="page-fit" data-l10n-id="page_scale_fit">По размеру страницы</option>
                      <option id="pageWidthOption" value="page-width" data-l10n-id="page_scale_width">По ширине страницы</option>
                      <option id="customScaleOption" value="custom"></option>
                      <option value="0.5">50%</option>
                      <option value="0.75">75%</option>
                      <option value="1">100%</option>
                      <option value="1.25">125%</option>
                      <option value="1.5">150%</option>
                      <option value="2">200%</option>
                    </select>
                  </span>
                </div>
              </div>
            </div>
          </div>
        </div>

        <menu type="context" id="viewerContextMenu">
          <menuitem label="Перейти на первую страницу" id="first_page" data-l10n-id="first_page"></menuitem>
          <menuitem label="Перейти на последнюю страницу" id="last_page" data-l10n-id="last_page"></menuitem>
          <menuitem label="Повернуть против часовой стрелки" id="page_rotate_ccw" data-l10n-id="page_rotate_ccw"></menuitem>
          <menuitem label="Повернуть по часовой стрелке" id="page_rotate_cw" data-l10n-id="page_rotate_cw"></menuitem>
        </menu>

        <div id="viewerContainer">
          <div id="viewer" contextmenu="viewerContextMenu"><a name="1"></a><div data-loaded="true" style="width: 80px; height: 74px;" class="page" id="pageContainer1"><canvas height="74" width="80" moz-opaque="" id="page1"></canvas><div class="textLayer"></div></div></div>
        </div>

        <div id="loadingBox" hidden="true">
          <div id="loading"></div>
          <div id="loadingBar"><div style="height: 100%; width: 100%;" class="progress full"></div></div>
        </div>

        <div id="errorWrapper" hidden="true">
          <div id="errorMessageLeft">
            <span id="errorMessage"></span>
            <button id="errorShowMore" onclick="" oncontextmenu="return false;" data-l10n-id="error_more_info">Детали</button>
            <button id="errorShowLess" onclick="" oncontextmenu="return false;" data-l10n-id="error_less_info" hidden="true">Скрыть детали</button>
          </div>
          <div id="errorMessageRight">
            <button id="errorClose" oncontextmenu="return false;" data-l10n-id="error_close">Закрыть</button>
          </div>
          <div class="clearBoth"></div>
          <textarea id="errorMoreInfo" readonly="readonly" hidden="true"></textarea>
        </div>
      </div> <!-- mainContainer -->

    </div> <!-- outerContainer -->
    <div id="printContainer"></div>
  


</body></html>